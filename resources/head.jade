- if (stationCodePath === undefined) stationCodePath = '';
meta(name="viewport" content="width=device-width, initial-scale=1")
- if (agent === 'ios')
	link(rel="apple-touch-icon" sizes="57x57" href=stationCodePath+"apple-touch-icon-57x57.png")
	link(rel="apple-touch-icon" sizes="60x60" href=stationCodePath+"apple-touch-icon-60x60.png")
	link(rel="apple-touch-icon" sizes="72x72" href=stationCodePath+"apple-touch-icon-72x72.png")
	link(rel="apple-touch-icon" sizes="76x76" href=stationCodePath+"apple-touch-icon-76x76.png")
	link(rel="apple-touch-icon" sizes="114x114" href=stationCodePath+"apple-touch-icon-114x114.png")
	link(rel="apple-touch-icon" sizes="120x120" href=stationCodePath+"apple-touch-icon-120x120.png")
	link(rel="apple-touch-icon" sizes="144x144" href=stationCodePath+"apple-touch-icon-144x144.png")
	link(rel="apple-touch-icon" sizes="152x152" href=stationCodePath+"apple-touch-icon-152x152.png")
	link(rel="apple-touch-icon" sizes="180x180" href=stationCodePath+"apple-touch-icon-180x180.png")
	meta(name="apple-mobile-web-app-title" content="trntxt")
- if (agent === 'android')
	link(rel="manifest" href=stationCodePath+"manifest.json")
- if (agent === 'ie' || agent === 'edge')
	meta(name="msapplication-config" content=stationCodePath+"browserconfig.xml")
	meta(name="application-name" content="trntxt")
- if (agent === 'other')
	link(rel="apple-touch-icon" sizes="72x72" href=stationCodePath+"apple-touch-icon-72x72.png")
